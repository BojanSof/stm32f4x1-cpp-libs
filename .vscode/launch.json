{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "STM32F401 Cortex Debug ST-link",
      "type": "cortex-debug",
      "request": "launch",
      "servertype": "stlink",
      "cwd": "${workspaceFolder}",
      "executable": "${command:cmake.launchTargetPath}",
      "device": "STM32F401CC",
      "v1": false,
      "interface": "swd",
      "svdFile": "${workspaceFolder}/svd/STM32F401.svd",
      "runToEntryPoint": "main"
    },
    {
      "name": "STM32F401 Cortex Debug JLink",
      "type": "cortex-debug",
