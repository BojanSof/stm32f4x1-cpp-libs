      "cwd": "${workspaceFolder}",
      "executable": "${command:cmake.launchTargetPath}",
      "servertype": "jlink",
      "device": "STM32F401CC",
      "interface": "swd",
      "svdFile": "${workspaceFolder}/svd/STM32F401.svd",
      "runToEntryPoint": "main"
    },
    {
      "name": "STM32F401 Cortex Debug CMSIS-DAPLink",
